#!/usr/bin/env bash
/home/modrisb/contributions/pijups/chirpha/chirpversion
ssh root@ha.c24 "rm -r /addons/chirp2mqtt"
ssh root@ha.c24 "mkdir /addons/chirp2mqtt"
